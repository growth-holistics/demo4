Dashboard aarki_embed_demo_landing_page_2 {
  title: 'Aarki Ecommerce Embedded Analytics'
  owner: 'thong.do@holistics.io'
  description: ''''''
  block t_35is: TextBlock {
    content: @md <!-- Aarki header -->
<div style="
  background: radial-gradient(1200px 500px at 10% 100%, rgba(208,255,113,0.20), transparent 55%),
              radial-gradient(1200px 500px at 90% 0%, rgba(39,234,166,0.18), transparent 50%),
              linear-gradient(135deg, #0F2B2C 0%, #193133 60%, #0F2B2C 100%);
  color:#E8FFF6; border:1px solid rgba(232,255,246,0.14); border-radius:16px;
  padding:22px 24px; ">
  <div style="display:flex; align-items:center; gap:16px; flex-wrap:wrap;">
    <div style="font-family: Urbanist, Inter, system-ui, -apple-system, Segoe UI, Roboto, Arial, sans-serif;
                font-weight:800; font-size:22px; letter-spacing:0.2px;">
      Aarki Performance Dashboard
    </div>
    <div style="margin-left:auto; display:flex; gap:8px; flex-wrap:wrap;">
      <span style="background:#004D43; color:#D0FF71; padding:6px 10px; border-radius:999px;
                   font-family: Inter, system-ui; font-size:12px; font-weight:600;">
        Live
      </span>
      <span style="background:rgba(39,234,166,0.15); border:1px solid rgba(39,234,166,0.45);
                   color:#27EAA6; padding:6px 10px; border-radius:999px;
                   font-family: Inter, system-ui; font-size:12px; font-weight:600;">
        Privacy-first Engagement
      </span>
    </div>
  </div>
  <div style="margin-top:6px; font-family: Inter, system-ui; font-size:13px; opacity:.9;">
    Driving ROI for leading publishers with compelling <span style="color:#27EAA6;">creative execution</span>.
  </div>
</div>
;;
  }
  block v_39rm: VizBlock {
    label: 'Companies (For Illustrative Purposes)'
    viz: DataTable {
      dataset: demo_ecommerce_version_2
      fields: [
        VizFieldFull {
          ref: r(ecommerce_merchants.id)
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: r(ecommerce_merchants.name)
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v_ne25: VizBlock {
    label: 'Users'
    viz: DataTable {
      dataset: demo_ecommerce_version_2
      fields: [
        VizFieldFull {
          ref: r(ecommerce_users.id)
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: r(ecommerce_users.first_name)
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: r(ecommerce_users.last_name)
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: r(ecommerce_users.email)
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v_kwnr: VizBlock {
    label: 'Total Users'
    viz: MetricKpi {
      dataset: demo_ecommerce_version_2
      value: VizFieldFull {
        ref: r(ecommerce_users.total_users)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      compare_value: VizPopSettings {
        field: r(ecommerce_users.sign_up_date)
        value: RelativePopValue {
          duration: 1
          granularity: 'month'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_color: '#004D43'
        }
        alignment: 'center'
        metric_value {
          font_color: '#004D43'
        }
      }
    }
    settings {
      hide_label: true
    }
    theme: block_white
  }
  block v_2n10: VizBlock {
    label: 'Total Products'
    viz: MetricKpi {
      dataset: demo_ecommerce_version_2
      value: VizFieldFull {
        ref: r(ecommerce_products.total_products)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_color: '#004D43'
        }
        alignment: 'center'
        metric_value {
