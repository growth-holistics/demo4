            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          // settings {
          //   color: '#008290FF'
          //   line_interpolation: 'smooth'
          // }
        }
      }
      settings {
        row_limit: 5000
        x_axis_show_null_datetime: true
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
    settings {
      hide_label: true
    }
  }
  block v_2y6r: VizBlock {
    label: 'Users'
