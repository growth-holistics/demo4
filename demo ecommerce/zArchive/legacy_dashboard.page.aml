            'category',
            'gender',
            'order_status',
            'item_category',
            'user_sign_up_date',
            'city'
          ]
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'user_sign_up_date'
      to: [
        CustomMapping {
          block: [
            'date',
            'country',
            'category',
            'gender',
            'order_status',
            'item_category',
            'order_date',
            'city'
          ]
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'city'
      to: [
        CustomMapping {
          block: [
            'date',
            'country',
            'category',
            'gender',
            'order_status',
            'item_category',
            'order_date',
            'user_sign_up_date'
          ]
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'city'
      to: [
        CustomMapping {
          block: 'country'
          field: ref('ecommerce_cities', 'name')
        }
      ]
    }
  ]
