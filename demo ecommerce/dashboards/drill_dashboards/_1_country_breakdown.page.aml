  block date_drill: DateDrillBlock {
    label: 'Date Drill'
  }
  block v1: VizBlock {
    label: 'Merchant x GMV'
    viz: PivotTable {
      dataset: demo_ecommerce
      rows: [
        VizFieldFull {
          label: 'Merchant'
          ref: ref('ecommerce_merchants', 'name')
          format {
            type: 'text'
          }
        }
      ]
      values: [
        VizFieldFull {
          ref: ref('order_master', 'revenue')
          format {
            type: 'number'
          }
        },
        VizFieldFull {
          ref: ref('order_master', 'revenue_delivered')
          format {
            type: 'number'
          }
        }
      ]
      settings {
        show_row_total: true
        show_column_total: true
        conditional_formats: [
          ConditionalFormat {
            ref: ref('order_master', 'revenue')
            format: SingleFormat {
              condition {
                operator: 'less_than'
                value: 20000
              }
              text_color: '#A50E43'
              background_color: '#FCB8B8'
            }
          },
          ConditionalFormat {
            ref: ref('order_master', 'revenue')
            format: SingleFormat {
              condition {
                operator: 'greater_than_equal'
                value: 20000
              }
              text_color: '#21908D'
              background_color: '#C3E8C2'
            }
          },
          ConditionalFormat {
            ref: ref('order_master', 'revenue_delivered')
            format: SingleFormat {
              condition {
                operator: 'less_than'
                value: 20000
              }
              text_color: '#A50E43'
              background_color: '#FCB8B8'
            }
          },
          ConditionalFormat {
            ref: ref('order_master', 'revenue_delivered')
            format: SingleFormat {
              condition {
                operator: 'greater_than_equal'
                value: 20000
              }
              text_color: '#21908D'
              background_color: '#C3E8C2'
            }
          }
        ]
      }
    }
  }
  block v2: VizBlock {
    label: 'Sales by City'
    viz: ColumnChart {
      dataset: demo_ecommerce
      x_axis: VizFieldFull {
        ref: ref('ecommerce_cities', 'name')
        format {
          type: 'text'
        }
      }
      y_axis {
