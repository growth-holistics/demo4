        }
      }
      series {
        field: VizFieldFull {
          label: 'Total '
          ref: ref('ecommerce_users', 'total_users')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
        settings {
          point {
            value: 'Male'
            color: '#F2B602'
          }
        }
      }
      settings {
        display_as_donut: true
        show_data_label: true
        show_percentage: true
      }
    }
  }
