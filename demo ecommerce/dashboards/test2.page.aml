  block filter_block: FilterBlock {
    label: 'This is a Filter Block'
    type: 'text'
    default {
      operator: 'is'
      value: 'default'
    }
  }
  block v1: VizBlock {
    label: 'Proportion of Ratings'
    viz: ColumnChart {
      dataset: movie_ratings_analysis
      filter {
        field: ref('public_movies', 'genres')
        operator: 'contains'
        value: 'Action'
      }
      x_axis: VizFieldFull {
        ref: ref('public_ratings', 'rating')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('public_ratings', 'rating')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
      }
    }
  }
  block v2: get_ratings_bar('Horror')

  view: CanvasLayout {
    label: 'View 1'
    height: 800
    block text_block {
      position: pos(40, 30, 1120, 420)
    }
    block filter_block {
      position: pos(40, 460, 400, 80)
    }
    block v1 {
      position: pos(40, 560, 400, 300)
    }
    block v2 {
      position: pos(440, 560, 400, 300)
    }
  }

