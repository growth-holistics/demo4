Func get_ratings_bar(genre: String) {
  VizBlock {
    label: 'Proportion of Ratings for Genre: ${genre}'
    viz: ColumnChart {
      dataset: movie_ratings_analysis
      filter {
        field: ref('public_movies', 'genres')
        operator: 'contains'
        value: '${genre}'
      }
      x_axis: VizFieldFull {
        ref: ref('public_ratings', 'rating')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      y_axis {
        settings {
          show_data_label_by: 'value'
        }
        series {
          field {
            ref: ref('public_ratings', 'rating')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
      }
    }
  }
}

