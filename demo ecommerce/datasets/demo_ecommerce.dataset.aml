PreAggregate agg_daily {
  dimension timestamp {
    for: ref('order_master','order_created_at')
    time_granularity: 'day'
  }

  measure order_count {
    for: ref('order_master','order_id')
    aggregation_type: 'count'
  }

  persistence: IncrementalPersistence {
    schema: 'persisted'
    incremental_column: 'timestamp'
  }
}

