    // relationship(users_cities, true),
    // relationship(order_master_merchants, true),
    // relationship(order_master_orders, true),
    // relationship(order_master_products, true)
  ]

  // relationships: [
  //   rel(rel_expr: orders.order_created_date > dim_dates.date_d, active: true),
  //   rel(rel_expr: order_master.user_id - orders.user_id, active: true),
  //   rel(rel_expr: products.category_id > map_categories.category_id, active: true),
  //   rel(rel_expr: users.city_id > cities.id, active: true),
  //   rel(rel_expr: order_master.product_id > products.id, active: true),
