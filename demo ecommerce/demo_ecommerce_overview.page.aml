  block v14: VizBlock {
    label: 'Orders by Country Name and Status'
    viz: ColumnChart {
      dataset: demo_ecommerce
      calculation f_7be4e9b {
        label: 'Status'
        formula: @aml  case(
  when: ecommerce_orders.status == 'cancelled', then: 'Cancelled',
  when: ecommerce_orders.status == 'delivered', then: 'Delivered',
  when: ecommerce_orders.status == 'refunded', then: 'Refunded'
  , else: 'others'
);;
        calc_type: 'dimension'
        data_type: 'text'
