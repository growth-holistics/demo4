Model ecommerce_countries {
  type: 'table'
  label: 'Countries'
  description: ''
  data_source_name: 'demodb'
  table_name: '"ecommerce"."countries"'

  dimension code {
    label: 'Country Code'
    type: 'text'
  }
  dimension name {
    label: 'Country Name'
    type: 'text'
  }
  dimension continent_name {
    label: 'Continent Name'
    type: 'text'
  }
  dimension map_search {
    label: 'Map Search'
    type: 'text'
    definition: @aql concat(
      'https://www.google.com/maps/search/',ecommerce_countries.name
    );;
  }

  //   dimension open_ai_search {
