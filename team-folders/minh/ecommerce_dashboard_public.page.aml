Dashboard ecommerce_dashboard_public {
  title: 'Ecommerce Dashboard'
  view: CanvasLayout {
    label: 'View 1'
    width: 1560
    height: 1080
    block v1 {
      position: pos(350, 60, 280, 120)
    }
    block v2 {
      position: pos(670, 60, 240, 120)
    }
    block v3 {
      position: pos(980, 60, 280, 120)
    }
    block v4 {
      position: pos(1280, 60, 230, 120)
    }
    block v5 {
      position: pos(60, 230, 560, 240)
    }
    block v6 {
      position: pos(330, 500, 560, 250)
    }
    block v7 {
      position: pos(-20, 790, 560, 260)
    }
    block v8 {
      position: pos(920, 230, 610, 380)
    }
    block v9 {
      position: pos(920, 630, 610, 420)
    }
    block f1 {
      position: pos(20, 440, 240, 80)
    }
    block f2 {
      position: pos(20, 630, 240, 80)
    }
    block f3 {
      position: pos(20, 540, 240, 80)
    }
    block d1 {
      position: pos(730, 280, 160, 80)
    }
    block f4 {
      position: pos(1360, 230, 170, 80)
    }
    block t1 {
      position: pos(400, 200, 270, 1100)
      layer: -1
    }
    block t2 {
      position: pos(410, 130, 210, 110)
    }
    block t3 {
      position: pos(20, 990, 200, 70)
    }
    block t4 {
      position: pos(-20, 410, 360, 360)
      layer: -1
    }
    block t5 {
      position: pos(320, 30, 1240, 200)
      layer: -2
    }
    block t6 {
      position: pos(30, 40, 90, 130)
    }
  }

  // Visualization
  block v1: VizBlock {
    label: 'Gross Merchandise Value'
    viz: MetricKpi {
      dataset: demo_ecommerce
      value: VizFieldFull {
        label: 'Gross Merchandise Value'
        ref: ref('order_master', 'gmv')
        format {
          type: 'number'
          pattern: '[$$]#,###0.00,,"M"'
          group_separator: ','
          decimal_separator: '.'
        }
      }
      compare_value: VizPopSettings {
        field: ref('order_master', 'order_created_at')
        duration: 3
        granularity: 'month'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v2: VizBlock {
    label: 'Net Merchandise Value'
    viz: MetricKpi {
      dataset: demo_ecommerce
      value: VizFieldFull {
        label: 'Net Merchandise Value'
        ref: ref('order_master', 'nmv')
        format {
          type: 'number'
          pattern: '[$$]#,###0.00,,"M"'
          group_separator: ','
          decimal_separator: '.'
        }
      }
      compare_value: VizPopSettings {
        field: ref('order_master', 'order_created_at')
        duration: 3
        granularity: 'month'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v3: VizBlock {
    label: 'Average Order Value'
    viz: MetricKpi {
      dataset: demo_ecommerce
      value: VizFieldFull {
        label: 'Average Order Value'
        ref: ref('order_master', 'aov')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      compare_value: VizPopSettings {
        field: ref('order_master', 'order_created_at')
        duration: 3
        granularity: 'month'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v4: VizBlock {
    label: 'Registered Users'
    viz: MetricKpi {
      dataset: demo_ecommerce
      value: VizFieldFull {
        label: 'Registered Users'
        ref: ref('order_master', 'count_distinct_users')
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      compare_value: VizPopSettings {
        field: ref('ecommerce_users', 'sign_up_date')
        duration: 3
        granularity: 'month'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v5: VizBlock {
    label: 'Monthly NMV'
    viz: CombinationChart {
      dataset: demo_ecommerce
      x_axis: VizFieldFull {
        ref: ref('order_master', 'order_created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        series {
          mark_type: 'column'
          field {
            label: 'Net Merchandise Value'
            ref: ref('order_master', 'nmv')
            format {
              type: 'number'
              pattern: '[$$]#,###0.00,"K"'
              group_separator: ','
              decimal_separator: '.'
            }
          }
          settings {
            color: '#255DD4'
          }
        }
      }
      y_axis {
        settings {
          alignment: 'right'
          axis_min: 0
        }
        series {
          mark_type: 'line'
          field {
            label: 'Growth Rate'
            ref: ref('order_master', 'cancelled_order_ratio')
            format {
              type: 'number'
              pattern: '#,###%'
            }
          }
          settings {
            color: '#3BDDAAFF'
          }
        }
      }
      settings {
        show_data_points: true
        row_limit: 5000
        legend_label: 'right'
      }
    }
  }
  block v6: VizBlock {
    label: 'AOV Trend'
    viz: AreaChart {
      dataset: demo_ecommerce
      x_axis: VizFieldFull {
        ref: ref('order_master', 'order_created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        settings {
          scale: 'logarithmic'
        }
        series {
          field {
            ref: ref('order_master', 'aov')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#255DD4'
            line_interpolation: 'step'
          }
        }
      }
      tooltips: [
        VizTooltip {
          field {
            ref: ref('order_master', 'gmv')
            format {
              type: 'number'
              pattern: '#,###0.00,"K"'
            }
          }
        },
        VizTooltip {
          field {
            ref: ref('order_master', 'nmv')
            format {
              type: 'number'
              pattern: '#,###0.00,"K"'
            }
          }
        }
      ]
      settings {
        row_limit: 5000
        legend_label: 'hidden'
        pop {
          field: ref('order_master', 'order_created_at')
          duration: 3
          granularity: 'month'
          show_growth_rate: true
        }
      }
    }
  }
  block v7: VizBlock {
    label: 'Monthly Sign up Trend'
    viz: CombinationChart {
      dataset: demo_ecommerce
      x_axis: VizFieldFull {
        ref: ref('ecommerce_users', 'sign_up_date')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        series {
          mark_type: 'area'
          field {
            ref: ref('ecommerce_users', 'users_signed_up_count')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#255DD4'
            line_interpolation: 'smooth'
          }
        }
        series {
          mark_type: 'area'
          field {
            label: 'Users Purchased'
            ref: ref('order_master', 'user_id')
            aggregation: 'count distinct'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#3CDD92FF'
            line_interpolation: 'smooth'
          }
        }
      }
      settings {
        row_limit: 5000
        legend_label: 'top'
      }
    }
  }
  block v8: VizBlock {
    label: 'User Retention'
    viz: ScatterChart {
      dataset: demo_ecommerce_users
      x_col: VizFieldFull {
        label: 'NMV'
        ref: ref('user_facts_aggr', 'nmv')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      y_col: VizFieldFull {
        ref: ref('user_facts_aggr', 'days_from_last_order')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      group_col: VizFieldFull {
        ref: ref('ecommerce_users', 'age_group')
        format {
          type: 'text'
        }
      }
      label_col: VizFieldFull {
        ref: ref('ecommerce_countries', 'continent_name')
        format {
          type: 'text'
        }
      }
      settings {
        row_limit: 5000
      }
    }
  }
  block v9: VizBlock {
    label: 'Top selling product categories'
    viz: PivotTable {
      dataset: demo_ecommerce
      rows: [
        VizFieldFull {
          ref: ref('map_categories', 'category')
          format {
            type: 'text'
          }
        }
      ]
      columns: [
        VizFieldFull {
          label: 'Year'
          ref: ref('order_master', 'order_created_at')
          transformation: 'datetrunc quarter'
          format {
            type: 'date'
            pattern: 'yyyy Qq'
          }
        }
      ]
      values: [
        VizFieldFull {
          label: ' '
          ref: ref('order_master', 'nmv')
          format {
            type: 'number'
            pattern: '[$$]#,###0.00,"K"'
            group_separator: ','
            decimal_separator: '.'
          }
        }
      ]
      settings {
        row_height: 'multiple_lines'
        pagination_size: 100
        show_row_with_no_data: true
        conditional_formats: [
          ConditionalFormat {
            ref: ref('order_master', 'nmv')
            aggregation: 'custom'
            format: ScaleFormat {
              min {
                value: 0
                color: '#E8F6F5'
              }
              mid {
                value: 160000
                color: '#74C9C5'
              }
              max {
                value: 320000
                color: '#18A59E'
              }
            }
          }
        ]
        sorts: [
          SortSetting {
            field_index: 0
            direction: 'desc'
          }
        ]
      }
    }
  }

  // Control
  block f1: FilterBlock {
    label: 'Date Range'
    type: 'date'
    default {
      operator: 'last'
      value: '12'
      modifier: 'month'
    }
  }
  block f2: FilterBlock {
    label: 'Country'
    type: 'field'
    source: FieldFilterSource {
      dataset: demo_ecommerce
      field: ref('ecommerce_countries', 'name')
    }
    default {
      operator: 'is'
      value: []
    }
  }
  block f3: FilterBlock {
    label: 'Region'
    type: 'field'
    source: FieldFilterSource {
      dataset: demo_ecommerce
      field: ref('ecommerce_countries', 'continent_name')
    }
    default {
      operator: 'is'
      value: 'Europe'
    }
  }
  block d1: DateDrillBlock {
    label: 'Time Period'
    default: 'month'
  }
  block f4: FilterBlock {
    label: 'Gender'
    description: 'View User Retention by Gender'
    type: 'field'
    source: FieldFilterSource {
      dataset: demo_ecommerce_users
      field: ref('ecommerce_users', 'gender')
    }
    default {
      operator: 'is'
      value: []
    }
    settings {
      input_type: 'nullable-single'
    }
  }

  // Text
  block t1: TextBlock {
    content: @md
    <div style="width: 300px; height: 1090px; 
      background-color:#0B325B;">
    </div>
  ;;
  }
  block t2: TextBlock {
    content: @md 
    <h1 style="color:#FFFFFF;">
      Ecommerce Dashboard
    </h1>
  ;;
  }
  block t3: TextBlock {
    content: @md
    <center>
      <img src="https://cdn.holistics.io/logos/holistics-logo-white.svg" 
      alt="demo" width="300" height="35">
    </center>
  ;;
  }
  block t4: TextBlock {
    content: @md
    <div style="width: 300px; height: 325px; 
      background-color:white; 
      border: 1px solid #E1E3EA;
      box-shadow: rgba(0, 0, 0, 0.1) 0px 20px 25px -5px, rgba(0, 0, 0, 0.04) 0px 10px 10px -5px;">
    </div>
  ;;
  }
  block t5: TextBlock {
    content: @md 
    <div style="width: 1200px; height: 175px; 
      background-color:white; 
      border: 1.5px solid #E1E3EA;
      box-shadow: rgba(0.1, 0.1, 1, 0.08) 0 5px 15px 2px;"}
    </div>
  ;;
  }
