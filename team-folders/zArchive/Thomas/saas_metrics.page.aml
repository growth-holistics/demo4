  block t5: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v8: VizBlock {
    label: 'MRR PROGRESS'
    viz: GaugeChart {
      dataset: saas
      calculation mrr {
        label: 'Mrr'
        formula: @aql sum(saas_subscriptions.daily_mrr);;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation mrr_goal {
        label: 'Untitled Metric'
        formula: @aql mrr * 1.6;;
        calc_type: 'measure'
        data_type: 'number'
      }
      value: VizFieldFull {
        ref: 'mrr'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      max_value: VizFieldFull {
        ref: 'mrr_goal'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        label: 'MRR PROGRESS'
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t6: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v9: VizBlock {
    label: 'MRR Breakdown'
    viz: DataTable {
      dataset: saas
      calculation mrr {
        label: 'Mrr'
        formula: @aql sum(saas_subscriptions.daily_mrr);;
        calc_type: 'measure'
        data_type: 'number'
      }
      fields: [
        VizFieldFull {
          ref: ref('saas_subscriptions', 'subscription_type')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: 'mrr'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t7: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v10: VizBlock {
    label: 'QUARTERLY NET NEW MRR'
    viz: ColumnChart {
      dataset: saas
      calculation mrr {
        label: 'Untitled Metric'
        formula: @aql sum(saas_subscriptions.daily_mrr) | where(saas_subscriptions.status == 'active' and saas_subscriptions.subscription_type == 'new');;
        calc_type: 'measure'
