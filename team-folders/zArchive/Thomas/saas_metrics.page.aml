      dataset: saas
      calculation net_new_mrr {
        label: 'Net MRR'
        formula: @aql sum(saas_subscriptions.daily_mrr) | where(saas_subscriptions.status == 'active' and saas_subscriptions.subscription_type == 'upgrade');;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation net_mrr_previous {
        label: 'Previous Net MRR'
        formula: @aql net_new_mrr * 1.2;;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('saas_transactions', 'transaction_date')
        operator: 'last'
        value: '1'
        modifier: 'week'
      }
      value: VizFieldFull {
        label: 'UPGRADE MRR - WEEKLY'
        ref: 'net_new_mrr'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      compare_value: VizFieldFull {
        ref: 'net_mrr_previous'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_size: 12
          font_color: '#07326C'
        }
        alignment: 'left'
        metric_value {
          font_color: '#255DD4'
        }
      }
    }
  }
  block t2: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v5: VizBlock {
    label: 'CHURN MRR - WEEKLY'
    viz: MetricKpi {
      dataset: saas
      calculation net_new_mrr {
        label: 'Net MRR'
        formula: @aql coalesce(
  sum(saas_subscriptions.daily_mrr) 
  | where(saas_subscriptions.status == 'canceled' and saas_subscriptions.subscription_type == 'downgrade')
  , 0);;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation net_mrr_previous {
        label: 'Previous Net MRR'
        formula: @aql net_new_mrr * 1.2;;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('saas_transactions', 'transaction_date')
        operator: 'last'
        value: '1'
        modifier: 'week'
      }
      value: VizFieldFull {
        label: 'CHURN MRR - WEEKLY'
        ref: 'net_new_mrr'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      compare_value: VizFieldFull {
        ref: 'net_mrr_previous'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_size: 12
          font_color: '#07326C'
        }
        alignment: 'left'
        metric_value {
          font_color: '#255DD4'
        }
      }
    }
  }
  block t3: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v6: VizBlock {
    label: ' QUATERLY MRR'
    viz: ColumnChart {
      dataset: saas
      filter {
        field: ref('saas_subscriptions', 'status')
        operator: 'is'
        value: 'active'
      }
      x_axis: VizFieldFull {
        ref: ref('saas_transactions', 'transaction_date')
        transformation: 'datetrunc quarter'
        format {
          type: 'date'
          pattern: 'yyyy Qq'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            label: 'MRR'
            ref: ref('saas_subscriptions', 'daily_mrr')
            aggregation: 'sum'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color_palette: 0
          }
        }
      }
      settings {
        row_limit: 5000
        x_axis_show_null_datetime: false
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t4: TextBlock {
    content: @md <div style="
        width:100%;
        height:100%;
        border-radius: 4px;
        background: #fafafa;"
    </div>;;
  }
  block v7: VizBlock {
    label: 'QUARTERLY UPGRADE MRR'
