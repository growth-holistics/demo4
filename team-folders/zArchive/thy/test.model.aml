Model test {
  type: 'query'
  label: 'Test'
  owner: "thy.nh@holistics.io"
  description: ""
  data_source_name: 'demodb'

  query: @sql 
            SELECT {{ #ci.name }} as city_name,
                {{ #co.name }} as country_name
            FROM {{ #cities as ci }}
                LEFT JOIN {{ #countries as co }} ON {{ #co.code }} = {{ #ci.country_code }} 
            ;;
            models: [cities, countries]

  dimension city_name {
    label: 'City Name'
    type: 'text'
  }

  dimension country_name {
    label: 'Country Name'
    type: 'text'
  }
}
