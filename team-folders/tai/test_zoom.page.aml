Dashboard test_zoom_dashboard {
  title: 'test zoom'
  description: ''

  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      default_zoom: 'fit_to_width'
      label: 'Overview'
      width: 1700
      height: 2840
      grid_size: 20
      auto_expand_vertically: true

      mobile {
        mode: 'auto'
      }
      block t_ngql {
        position: pos(180, 140, 40, 60)
      }
    }
    tab tab2: CanvasLayout {
      default_zoom: 'fit_to_width'
      label: 'Games'
      width: 1700
      height: 610
      grid_size: 20
      auto_expand_vertically: true
      mobile {
        mode: 'auto'
      }
      block t_sygp {
        position: pos(120, 60, 40, 60)
      }
    }
    tab tab_l4op: CanvasLayout {
      default_zoom: 'fit_to_width'
      label: 'Players'
      width: 1700
      height: 1560
      grid_size: 20
      auto_expand_vertically: true
      mobile {
        mode: 'auto'
      }
      block t_eddj {
        position: pos(80, 60, 40, 60)
      }
    }
  }
  settings {
    timezone: 'Etc/UTC'
    allow_timezone_change: true
    cache_duration: 1440
  }
  block t_ngql: TextBlock {
    content: @md 1;;
  }
  block t_sygp: TextBlock {
    content: @md 2;;
  }
  block t_eddj: TextBlock {
    content: @md 1;;
  }
}