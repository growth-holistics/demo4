      layer: 1
    }
    block order_status {
      position: pos(0, 80, 300, 80)
      layer: 1
    }
    block item_category {
      position: pos(300, 80, 300, 80)
      layer: 1
    }
    block order_date {
      position: pos(600, 80, 300, 80)
      layer: 1
    }
    block user_sign_up_date {
      position: pos(900, 80, 300, 80)
      layer: 1
