  PageTheme MirkWood {
    background {
      bg_color: "#E3E0D7"
    }
    canvas {
      background {
        bg_color: '#F1EFEA'
      }
      border {
        border_radius: '16px'
      }
      shadow: "md"

    }
    block {
      label: FontTheme {
        font_size: 24
        font_color: '#4B3E18'
