PageTheme Classic {
  
    background {
      bg_color: "white"
    }
    canvas {
      background {
        bg_color: '#F7F8F9'
      }
      border {
        border_radius: '4px'
        border_width: 1
        border_color: '#E8E8E8'
      }
      shadow: "none"
    }
    block {
      padding: 12 
      border {
        border_width: 1
        border_color: '#E8E8E8'
        border_radius: 8
      }
      background {
        bg_color: "white"

      }
    }

  }

PageTheme HolisticsLight {
    background {
      bg_image: 'linear-gradient(to top, #CCEEDE, #F7FFFB'
    }
    canvas {
      background {
        bg_color: 'white'
      }
      border {
        border_width: 1.5
        border_radius: '16px'
        border_color: "#D1ECDF"
      }
      shadow: "none"

    }
    block {
      label: FontTheme {
        font_size: 12
        font_color: '#1357A0'
      }
      text {
        font_size: 12
        font_color: '#1357A0'
      }
      padding: 12
      border {
        border_width: 1
        border_color: '#D1E5FA'
        border_radius: 12
      }
      background {
        bg_image: 'linear-gradient(to top), #EBF2FF, #F6F9FF'

      }
    }

  }

