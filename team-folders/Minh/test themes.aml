PageTheme Macchiato {
    background {
      bg_color: "#2C241B"
    }
    canvas {
      background {
        bg_color: '#2C241B'
      }
      border {
        border_width: 5
        border_radius: 20
        border_style: "double"
        border_color: "#AF9A63"
      }
      shadow: "none"

    }
    block {
      padding: 12
      label: FontTheme {
        font_size: 12
        font_color: '#E6DBBC'
      }
      border {
        border_radius: 4
      }
      text {
        font_size: 12
        font_color: '#E6DBBC'
      }
      
      background {
        bg_image: 'linear-gradient(to top, #4E3E2C, #2C241B'  

      }
    }

  }


BlockTheme Clear {
  padding: 1
  border {
    border_style: "none"
    border_color: "transparent"
  }
  background {
    bg_color: 'none'
    bg_image: "none"
  }
}