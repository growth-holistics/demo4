        label: 'Profits'
        formula: @aql ecommerce_products.price-ecommerce_products.cost;;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation f_5e84d48 {
        label: 'Amazon Search'
        formula: @aql concat('https://www.amazon.com/s?k=',ecommerce_products.name);;
        calc_type: 'measure'
        data_type: 'text'
        hidden: true
      }
      calculation f_6d33d70 {
        label: 'Google Search'
        formula: @aql concat('https://www.google.com/search?q=',ecommerce_merchants.name);;
        calc_type: 'measure'
        data_type: 'text'
        hidden: true
      }
      calculation f_ac65b5c {
        label: 'Map Search'
        formula: @aql concat('https://www.google.com/maps/search/',ecommerce_merchants.name);;
        calc_type: 'measure'
        data_type: 'text'
        hidden: true
      }
      fields: [
        VizFieldFull {
          ref: ref('ecommerce_products', 'name')
          format {
            type: 'text'
          }
          action a_1: VizAction {
            type: 'GoToUrl'
            label: 'Amazon Search'
            url: ActionFieldInput {
              ref: 'f_5e84d48'
            }
          }
          uname: 'ecommerce_products_name'
        },
        VizFieldFull {
          label: 'Merchant Name'
          ref: ref('ecommerce_merchants', 'name')
          format {
            type: 'text'
          }
          action a_1: VizAction {
            type: 'GoToUrl'
            label: 'Google Search'
            url: ActionFieldInput {
              ref: 'f_6d33d70'
            }
          }
          action a_2: VizAction {
            type: 'GoToUrl'
            label: 'Map Search'
            url: ActionFieldInput {
              ref: 'f_ac65b5c'
            }
          }
          uname: 'ecommerce_merchants_name'
        },
        VizFieldFull {
          label: 'Revenue'
          ref: ref('order_master', 'revenue_delivered')
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'custom_revenue_delivered'
        },
        VizFieldFull {
          label: 'AOV'
          ref: ref('order_master', 'aov')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_products', 'price')
          format {
            type: 'number'
            pattern: '[$$]#,###'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_products', 'cost')
          format {
            type: 'number'
            pattern: '[$$]#,###'
          }
        },
        VizFieldFull {
          label: 'Completed Orders'
          ref: ref('order_master', 'completed_order_count')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          label: 'Refunded Orders'
          ref: ref('order_master', 'refunded_orders_count')
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'custom_refunded_orders_count'
        },
        VizFieldFull {
          label: 'Cancelled Orders'
          ref: ref('order_master', 'cancelled_orders_count')
          format {
            type: 'number'
            pattern: '#,###'
          }
          uname: 'custom_cancelled_orders_count'
        }
      ]
      settings {
        row_height: 'multiple_lines'
        show_row_number: true
        conditional_formats: [
          ConditionalFormat {
            key: 'custom_refunded_orders_count'
            format: SingleFormat {
              condition {
                operator: 'greater_than_equal'
                value: 5
              }
              text_color: '#BF0E08'
              background_color: '#FAD2D1'
            }
          },
          ConditionalFormat {
            key: 'custom_cancelled_orders_count'
            format: SingleFormat {
              condition {
                operator: 'greater_than_equal'
                value: 5
              }
              text_color: '#BF0E08'
              background_color: '#FAD2D1'
            }
          },
          ConditionalFormat {
            key: 'custom_revenue_delivered'
            format: SingleFormat {
              condition {
                operator: 'greater_than_equal'
                value: 1500
              }
              text_color: '#328159'
              background_color: '#B5E3CD'
            }
          }
        ]
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        frozen_columns: 2
        column_styles: [
          ColumnStyle {
            key: 'ecommerce_products_name'
            width: 157
