        ref: 'untitled_metric_1681e8f'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        reverse_color: true
        display_mode: 'progress'
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_size: 15
        }
        alignment: 'center'
        metric_value {
          font_size: 45
          font_color: '#156064FF'
        }
        progress {
          bar {
            color: '#156064FF'
          }

        }
      }
    }
  }
  block v13: VizBlock {
    label: 'GMV by Age Group - Gender'
    viz: PivotTable {
      dataset: demo_ecommerce
      rows: [
        VizFieldFull {
          ref: ref('ecommerce_countries', 'continent_name')
          format {
            type: 'text'
