Dashboard Travel_Agent {
  title: 'Travel Agent'
  description: ''''''

  theme: PageTheme {
    background {
      bg_color: "#FFFFFF"
      bg_repeat: false
      bg_size: "cover"
      // bg_image:
    }
    canvas {
      border {
        border_radius: 10
        border_width: 1
        border_color: '#E8E8E8'
        border_style: "solid"
      }
      background {
        bg_color: 'green'
        bg_repeat: true
        bg_size: "contain"
        // bg_image:
      }
      shadow: "none"
      opacity: 1
    }
    block {
      label {
        font_family: "Inter"
        font_size: 14
        font_color: "#53586A"
        font_weight: "medium"
        font_style: "normal"
      }
      text {
        font_family: "Inter"
        font_size: 12
        font_color: "#53586A"
        font_weight: "normal"
        font_style: "normal"
      }
      border {
        border_width: 1
        border_radius: 8
        border_color: '#E8E8E8'
        border_style: "solid"
      }
      background {
        bg_color: "#FFFFFF"
        bg_repeat: false
        bg_size: "cover"
        // bg_image:
      }
      padding: 12
      shadow: "none"
      opacity: 1
    }
  }

  view: CanvasLayout {
    label: 'View 1'
    height: 800
    grid_size: 20
    block v4 {
      position: pos(40, 200, 1140, 300)
    }
  }
  block v4: VizBlock {
    label: 'Customer List'
    viz: DataTable {
      dataset: demo_ecommerce
      fields: [
        VizFieldFull {
          ref: ref('ecommerce_users', 'full_name')
          format {
            type: 'text'
          }
          action a_1: VizAction {
            type: 'GoToUrl'
            label: 'Linkedin Search'
            url: ActionFieldInput {
              ref: ref('ecommerce_users', 'linkedin_search')
              aggregation: 'min'
            }
          }
          action a_2: VizAction {
            type: 'GoToUrl'
            label: 'Email Customer'
            url: ActionFieldInput {
              ref: ref('ecommerce_users', 'email_user')
              aggregation: 'min'
            }
          }
          action a_3: VizAction {
            type: 'GoToUrl'
            label: 'Call Customer'
            url: ActionFieldInput {
              ref: ref('ecommerce_users', 'phone')
              aggregation: 'min'
            }
          }
          action a_4: VizAction {
            type: 'GoToUrl'
            label: 'Write Recommendation'
            url: ActionFieldInput {
              ref: ref('order_master', 'recommendation')
              aggregation: 'min'
            }
          }
        },
        VizFieldFull {
          label: 'City Destination'
          ref: ref('ecommerce_cities', 'name')
          format {
            type: 'text'
          }
          action a_1: VizAction {
            type: 'GoToUrl'
            label: 'Recommendation based on Location, Age Demographic and Gender'
            url: ActionFieldInput {
              ref: ref('order_master', 'open_ai_search')
              aggregation: 'min'
            }
          }
          action a_2: VizAction {
            type: 'GoToUrl'
            label: 'Google Map Search'
            url: ActionFieldInput {
              ref: ref('ecommerce_cities', 'map_search')
              aggregation: 'min'
            }
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_users', 'age_group')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_users', 'gender')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 50
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
}