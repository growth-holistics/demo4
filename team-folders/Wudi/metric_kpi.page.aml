  block v6: VizBlock {
    label: 'Product Listings'
    viz: DataTable {
      dataset: pokedex
      fields: [
        VizFieldFull {
          ref: ref('sample_pokemon_gif', 'pokemon')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('sample_pokemon_gif', 'description')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
