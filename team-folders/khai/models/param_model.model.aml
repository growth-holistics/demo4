Model param_model {
  type: 'query'
  label: 'Param Model'
  description: ''
  data_source_name: 'demodb'
  dimension column {
    label: '?column?'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.?column? }};;
  }

  param metric_selection {
    label: 'Metric Selection'
    type: 'text'
    allowed_values: ['Revenue', 'Total Orders', 'Total Users']
  }

  param dim_choice {
    label: 'Dim Choice'
    type: 'text'
    allowed_values: ['Countries', 'Cities', 'Gender']
  }

  param time_grain {
    label: 'Time Grain'
    type: 'text'
    allowed_values: ['Month', 'Quarter', 'Year']
  }

  param number_selection {
    label: 'Number Selection'
    type: 'number'
  }

  owner: 'khai.to@holistics.io'
  query: @sql select 1;;
  models: [
  ]
}
