Dashboard test_bug {
  title: '[Demo] Ecommerce Overview 4.0'
  description: ''''''


  block hd1: TextBlock {
    content: @md <center>
  <picture>
    <source media="(min-width:650px)" srcset="https://cdn.holistics.io/logos/holistics-logo-white.svg">
    <img src="holisticswhitelabeldemo.jpg" alt="demo" width="300
    " height="35">
  </picture>
</center>;;
  }
  block hd2: TextBlock {
    content: @md <h1 style="color:#FFFFFF;">Ecommerce Dashboard</h1>;;
  }
  block hd3: TextBlock {
    content: @md <div style="width: 1250px; height: 200px; background-image: linear-gradient(45deg, #0B325B,#0B325B, #1B7CE4);"></div>;;
  }
  block t2: TextBlock {
    content: @md <div style="color:white;">
<p>
This dashboard tracks the KPIs of all managers for their respective teams. This will also be presented in our monthly presentation to our board and investors. If you have questions about the data here, please contact the <a href ="mailto:data@holistics.io"> data team </a>.
</p>
</div>;;
  }
  block v1: VizBlock {
    label: ' KPI Overview'
    viz: MetricSheet {
      dataset: demo_ecommerce
      filter {
        field: ref('dim_dates', 'date_key')
        operator: 'matches'
        value: '12 months ago to last month end'
      }
      date_field: VizFieldFull {
        ref: ref('dim_dates', 'date_key')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      rows: [
        MetricHeading {
          label: 'ðŸŽ¯Orders'
          settings {
            background_color: '#255DD4'
            text_color: '#E9EFFB'
          }
        },
        MetricSeries {
          field {
            ref: ref('ecommerce_orders', 'total_orders_count')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricSeries {
          field {
            ref: ref('ecommerce_orders', 'delivered_orders_count')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricSeries {
          field {
            ref: ref('ecommerce_orders', 'cancelled_orders_count')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricSeries {
          field {
            ref: ref('ecommerce_orders', 'refunded_orders_count')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricSeries {
          field {
            ref: ref('order_master', 'aov')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricHeading {
          label: 'ðŸ’° Revenue'
          settings {
            background_color: '#255DD4'
            text_color: '#E9EFFB'
          }
        },
        MetricSeries {
          field {
            ref: ref('order_master', 'nmv')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        },
        MetricSeries {
          field {
            ref: ref('order_master', 'gmv')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        max_column: 3
      }
    }
    settings {
      hide_label: true
      hide_controls: true
    }
  }
  block v2: VizBlock {
    label: 'GMV'
    viz: MetricKpi {
      dataset: demo_ecommerce
      calculation f_2fb1cc3 {
        label: 'GMT last year'
        formula: @aml order_master.gmv * 0.87;;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('order_master', 'order_created_at')
        operator: 'matches'
        value: '2023'
      }
      value: VizFieldFull {
        label: 'Gross Merchandise Value'
        ref: ref('order_master', 'gmv')
        aggregation: 'custom'
        format {
          type: 'number'
          pattern: '[$$]#,###'
        }
      }
      compare_value: VizPopSettings {
        field: ref('order_master', 'order_created_at')
        duration: 1
        granularity: 'year'
      }
    }
    settings {
      hide_label: true
      hide_controls: true
    }
  }
  block v2_dup_dup: VizBlock {
    label: 'Total Orders by Country'
    viz: FilledMap {
      dataset: demo_ecommerce
      filter {
        field: ref('ecommerce_countries', 'name')
        operator: 'is_not'
        value: [
          'United States',
          'United Kingdom',
          'Germany',
          'France'
        ]
      }
      location {
        field: VizFieldFull {
          ref: ref('ecommerce_countries', 'name')
          format {
            type: 'text'
          }
        }
        primary_field: true
      }
      value: VizFieldFull {
        ref: ref('order_master', 'gmv')
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      settings {
        value_colors {
          min {
            value: 6000000
            color: '#A50E43'
          }
          mid {
            color: '#255DD4'
          }
          max {
            value: 8200000
            color: '#18A59E'
          }
        }
      }
    }
  }
  block v6: VizBlock {
    label: 'GMV by Category and Country'
    viz: PivotTable {
      dataset: demo_ecommerce
      filter {
        field: ref('order_master', 'order_created_at')
        operator: 'matches'
        value: '2023'
      }
      rows: [
        VizFieldFull {
          ref: ref('ecommerce_countries', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('map_categories', 'parent_category')
          format {
            type: 'text'
          }
        }
      ]
      values: [
        VizFieldFull {
          label: 'GMV'
          ref: ref('order_master', 'gmv')
          aggregation: 'custom'
          format {
            type: 'number'
            pattern: '[$$]#,###'
          }
        },
        VizFieldFull {
          label: 'NMV'
          ref: ref('order_master', 'nmv')
          aggregation: 'custom'
          format {
            type: 'number'
            pattern: '[$$]#,###'
          }
        },
        VizFieldFull {
          label: 'AOV'
          ref: ref('order_master', 'aov')
          aggregation: 'custom'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        pagination_size: 100
        show_column_total: true
        conditional_formats: [
          ConditionalFormat {
            ref: ref('order_master', 'aov')
            aggregation: 'custom'
            format: ScaleFormat {
              min {
                value: 100
                color: '#E8F6F5'
              }
              max {
                value: 3000
                color: '#18A59E'
              }
            }
          }
        ]
      }
    }
  }
  block v8: VizBlock {
    label: 'Users vs Orders Growth'
    viz: CombinationChart {
      dataset: demo_ecommerce
      x_axis: VizFieldFull {
        ref: ref('ecommerce_users', 'sign_up_date')
        transformation: 'datetrunc quarter'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('ecommerce_users', 'total_users')
            aggregation: 'custom'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#18A59E'
          }
        }
      }
      y_axis {
        settings {
          alignment: 'right'
        }
        series {
          mark_type: 'line'
          field {
            ref: ref('order_master', 'delivered_orders_count')
            aggregation: 'custom'
            format {
              type: 'number'
            }
          }
          settings {
            color: '#255DD4'
          }
        }
      }
    }
  }
  block v10: VizBlock {
    label: 'AOV'
    viz: MetricKpi {
      dataset: demo_ecommerce
      calculation f_2fb1cc3 {
        label: 'GMT last year'
        formula: @aml order_master.gmv * 0.87;;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('order_master', 'order_created_at')
        operator: 'matches'
        value: '2023'
      }
      value: VizFieldFull {
        label: 'Average Order Value'
        ref: ref('order_master', 'aov')
        aggregation: 'custom'
        format {
          type: 'number'
          pattern: '[$$]#,###'
        }
      }
      compare_value: VizPopSettings {
        field: ref('order_master', 'order_created_at')
        duration: 1
        granularity: 'year'
      }
    }
    settings {
      hide_label: true
      hide_controls: true
    }
  }
  block v12: VizBlock {
    label: 'Gross Merchandise Value'
    viz: AreaChart {
      dataset: demo_ecommerce
      calculation f_2fb1cc3 {
        label: 'GMT last year'
        formula: @aml order_master.gmv * 0.87;;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('order_master', 'order_created_at')
        operator: 'matches'
        value: '2023'
      }
      x_axis: VizFieldFull {
        ref: ref('order_master', 'order_created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        series {
          field {
            label: ''
            ref: ref('order_master', 'gmv')
            aggregation: 'custom'
            format {
              type: 'number'
              pattern: '#,###'
            }
          }
          settings {
            color: '#18A59E'
          }
        }
      }
      settings {
        legend_label: 'hidden'
      }
    }
    settings {
      hide_controls: true
    }
  }
  block t3: TextBlock {
    content: @md <hr style="border: 1px solid; opacity:20%">;;
  }
  block v13: VizBlock {
    label: 'GMV by Product Category'
    viz: CustomChart {
      dataset: demo_ecommerce
      custom_chart_id: 7466
      field source: VizFieldFull {
        ref: ref('map_categories', 'parent_category')
        format {
          type: 'text'
        }
      }
      field target: VizFieldFull {
        ref: ref('map_categories', 'category')
        format {
          type: 'text'
        }
      }
      field volume: VizFieldFull {
        ref: ref('order_master', 'gmv')
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      setting margin_top: 30
      setting node_align: 'left'
      setting node_width: 15
      setting margin_left: 30
      setting margin_right: 30
      setting node_padding: 10
      setting margin_bottom: 30
    }
  }
  block f1: FilterBlock {
    label: 'Country'
    type: 'field'
    source: FieldFilterSource {
      dataset: demo_ecommerce
      field: ref('ecommerce_countries', 'name')
    }
    default {
      operator: 'is'
      value: []
    }
    settings {
      input_type: 'nullable-single'
    }
  }
  block t5: TextBlock {
    content: @md <h1>Overview</h1>;;
  }
  block t7: TextBlock {
    content: @md Click on Pivot table to Cross-filter Sankey Chart;;
  }
  block t8: TextBlock {
    content: @md <h1>User Analysis</h1>;;
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 2710
    block hd1 {
      position: pos(830, 40, 330, 100)
    }
    block hd2 {
      position: pos(30, 30, 630, 60)
    }
    block hd3 {
      position: pos(0, 0, 1210, 180)
      layer: -1
    }
    block t2 {
      position: pos(30, 90, 630, 80)
    }
    block v2 {
      position: pos(650, 250, 270, 180)
    }
    block v10 {
      position: pos(900, 250, 270, 180)
    }
    block v12 {
      position: pos(30, 250, 560, 180)
      layer: -1
    }
    block t5 {
      position: pos(30, 190, 390, 60)
    }
  }
}