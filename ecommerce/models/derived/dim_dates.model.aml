Model dim_dates {
  type: 'query'
  label: 'Dim Dates'
  description: ''
  data_source_name: 'demodb'
  dimension date_key {
    label: 'Date Key'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.report_date }};;
  }

  owner: 'ha.pham+demo4@holistics.io'
  query: @sql
    SELECT date_key
    FROM  generate_series ('2015-07-01' :: date, current_date ,'1 day' :: interval) date_key;;
  models: [
  ]
}
