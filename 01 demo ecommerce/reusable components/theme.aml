PageTheme ticketing_concert_theme {
  background {
    bg_color: "#FFD1DC"  // Light pink
    bg_image: "https://variety.com/wp-content/uploads/2024/02/Screen-Shot-2024-02-28-at-12.40.18-PM.png?w=1000&h=667&crop=1"  // You'd need to replace this with an actual image URL
    bg_repeat: false
    bg_size: "cover"
  }
  canvas {
    border {
      border_radius: 30
      border_width: 1
      border_color: '#FF69B4'  // Hot pink
      border_style: "solid"
    }
    background {
      bg_color: 'rgba(255, 182, 193, 0.7)'  // Light pink with some transparency
      bg_repeat: false
      bg_size: "cover"
    }
    shadow: "lg"
    opacity: 0.9
  }
  block {
    label {
      font_family: "Playfair Display, serif"  // A more elegant font
      font_size: 15
      font_color: "#FF1493"  // Deep pink
      font_weight: "bold"
      font_style: "normal"
    }
    text {
      font_family: "Roboto, sans-serif"
      font_size: 12
      font_color: "#4B0082"  // Indigo
      font_weight: "normal"
      font_style: "normal"
    }
    border {
      border_width: 1
      border_radius: 8
      border_color: '#C71585'  // Medium violet red
      border_style: "none"
    }
    background {
      bg_color: "rgba(255, 255, 255, 0.8)"  // Slightly transparent white
      bg_repeat: false
      bg_size: "cover"
    }
    padding: 8
    shadow: "none"
    opacity: 1
  }
}

