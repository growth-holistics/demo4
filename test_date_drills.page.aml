Dashboard test_date_drills {
  title: 'Test Date Drills'
  block title: TextBlock {
    content: @md # Test Date Drills;;
  }
  block v1: VizBlock {
    label: 'GMV - Gross Merchandise Value by Month Created Date'
    viz: LineChart {
      dataset: demo_ecommerce_version_2
      x_axis: VizFieldFull {
        ref: ref('ecommerce_orders', 'created_date')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: 'gmv'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color_palette: 0
          }
        }
      }
      settings {
        row_limit: 5000
        x_axis_show_null_datetime: false
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'GMV - Table'
    viz: DataTable {
      dataset: demo_ecommerce_version_2
      fields: [
        VizFieldFull {
          ref: ref('ecommerce_orders', 'created_date')
          transformation: 'datetrunc year'
          format {
            type: 'date'
            pattern: 'yyyy'
          }
        },
        VizFieldFull {
          ref: 'gmv'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v3: VizBlock {
    label: 'GMV - Gross Merchandise Value by Month Created Date'
    viz: PivotTable {
      dataset: demo_ecommerce_version_2
      rows: [
        VizFieldFull {
          ref: ref('ecommerce_orders', 'created_date')
          transformation: 'datetrunc year'
          format {
            type: 'date'
            pattern: 'yyyy'
          }
        }
      ]
      columns: [
        VizFieldFull {
          ref: ref('ecommerce_countries', 'name')
          format {
            type: 'text'
          }
        }
      ]
      values: [
        VizFieldFull {
          ref: 'gmv'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_total: true
        show_column_total: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v4: VizBlock {
    label: 'GMV - KPI'
    viz: MetricKpi {
      dataset: demo_ecommerce_version_2
      filter {
        field: ref('ecommerce_orders', 'created_date')
        operator: 'matches'
        value: 'this year'
      }
      value: VizFieldFull {
        ref: 'gmv'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'left'
      }
    }
  }
  block v5: VizBlock {
    label: 'GMV - Gross Merchandise Value by Month Created Date'
    viz: PieChart {
      dataset: demo_ecommerce_version_2
      legend: VizFieldFull {
        ref: ref('ecommerce_orders', 'created_date')
        transformation: 'datetrunc year'
        format {
          type: 'date'
          pattern: 'yyyy'
        }
      }
      series {
        field: VizFieldFull {
          ref: 'gmv'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
        settings {
          color_palette: 0
        }
      }
      settings {
        row_limit: 5000
        data_label_position: 'none'
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v6: VizBlock {
    label: 'GMV - Gross Merchandise Value by Month Created Date'
    viz: PieChart {
      dataset: demo_ecommerce_version_2
      filter {
        field: ref('ecommerce_orders', 'created_date')
        operator: 'matches'
        value: 'this year'
      }
      legend: VizFieldFull {
        ref: ref('ecommerce_countries', 'name')
        format {
          type: 'text'
        }
      }
      series {
        field: VizFieldFull {
          ref: 'gmv'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
        settings {
          color_palette: 0
        }
      }
      settings {
        row_limit: 5000
        data_label_position: 'none'
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v7: VizBlock {
    label: 'GMV - Gross Merchandise Value by Month Created Date'
    viz: MetricSheet {
      dataset: demo_ecommerce_version_2
      date_field: VizFieldFull {
        ref: ref('ecommerce_orders', 'created_date')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      rows: [
        MetricSeries {
          field: VizFieldFull {
            ref: 'gmv'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  interactions: []
  view: CanvasLayout {
    label: 'View 1'
    width: 1220
    height: 1680
    grid_size: 20
    block v1 {
      position: pos(20, 80, 580, 360)
      layer: 1
    }
    block v2 {
      position: pos(20, 460, 580, 360)
      layer: 1
    }
    block v3 {
      position: pos(620, 460, 580, 360)
      layer: 1
    }
    block v4 {
      position: pos(620, 80, 580, 360)
      layer: 1
    }
    block v5 {
      position: pos(20, 840, 580, 360)
      layer: 1
    }
    block v6 {
      position: pos(620, 840, 580, 360)
      layer: 1
    }
    block v7 {
      position: pos(20, 1220, 1180, 360)
      layer: 1
    }
    block title {
      position: pos(20, 20, 1160, 60)
    }
    mobile {
      mode: 'auto'
    }
  }
  theme: H.themes.vanilla
}