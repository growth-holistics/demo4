  block v_mbgn: VizBlock {
    label: 'QTD (vs YoY)'
    viz: MetricKpi {
      dataset: demo_ecommerce
      filter {
        field: r(ecommerce_orders.created_at)
        operator: 'matches'
        value: 'this quarter begin to today'
      }
      value: VizFieldFull {
        label: 'Orders'
        ref: r(ecommerce_orders.total_orders_count)
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      compare_value: VizPopSettings {
        field: r(ecommerce_orders.created_at)
