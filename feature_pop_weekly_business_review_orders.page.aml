            ref: r(ecommerce_orders.total_orders_count)
            format {
              type: 'number'
              pattern: '#,###'
            }
          }
          settings {
            color_palette: 0
          }
        }
      }
      settings {
        pop {
          field: r(ecommerce_orders.created_at)
          value: RelativePopValue {
            duration: 12
            granularity: 'month'
          }
          show_growth_rate: true
        }
      }
    }
  }
  block v_hr4v: VizBlock {
