Dashboard feature_pop_weekly_business_review_orders {
  title: '[Feature] PoP - Weekly Business Review Orders'
  owner: 'chukwudi@holistics.io'
  block date_drill: DateDrillBlock {
    label: 'Date Drill'
  }
  block compare_against_period: PopBlock {
    label: 'Compare Against Period'
  }
  block v_o3ru: VizBlock {
    label: 'Last 6 weeks (vs preceeding)'
    viz: LineChart {
      dataset: demo_ecommerce
      filter {
        field: r(ecommerce_orders.created_at)
        operator: 'last'
        value: 6
        modifier: 'week'
      }
      x_axis: VizFieldFull {
        ref: r(ecommerce_orders.created_at)
        transformation: 'datetrunc week'
        format {
          type: 'date'
          pattern: 'wwww'
        }
      }
      y_axis {
        settings {
          show_data_label_by: 'value'
        }
        series {
          field: VizFieldFull {
            ref: r(ecommerce_orders.total_orders_count)
            format {
              type: 'number'
              pattern: '#,###'
            }
          }
          settings {
            color_palette: 0
          }
        }
      }
      settings {
        pop {
          field: r(ecommerce_orders.created_at)
          value: RelativePopValue {
            duration: 6
            granularity: 'week'
          }
          show_growth_rate: true
        }
      }
    }
  }
  block v_29aa: VizBlock {
    label: 'Last 12 months (YoY)'
    viz: LineChart {
      dataset: demo_ecommerce
      filter {
        field: r(ecommerce_orders.created_at)
        operator: 'last'
        value: 12
        modifier: 'month'
      }
      x_axis: VizFieldFull {
        ref: r(ecommerce_orders.created_at)
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
