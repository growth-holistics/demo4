Model location {
  type: 'query'
  label: 'location'
  description: ''
  data_source_name: 'demodb'
  owner: 'thy.nh@holistics.io'

  models: [
    cities,
    countries
  ]

  query: @sql
          SELECT {{ #ci.name }} as city_name,
              {{ #co.name }} as country_name
          FROM {{ #cities as ci }}
          LEFT JOIN {{ #countries as co }} 
          ON {{ #co.code }} = {{ #ci.country_code }} 
  ;;

  dimension city_name {
    label: 'City Name'
    type: 'text'
  }
  dimension country_name {
    label: 'Country Name'
    type: 'text'
  }
}





